# For convenience of debugging: makes an assembler file.

export GAMEDATA=~/java/classes/org/sc3d/apt/crazon/gamedata/
mkdir $GAMEDATA

java org.sc3d.apt.crazon.compiler.Flattener ../wiki/ Rocks >$GAMEDATA/asm.nfk
echo "#Generated by compiler/makeAsm, do not edit.
name: Rocks
width: 256
height: 256
msPerFrame: 25
" > $GAMEDATA/properties.txt
ls -1 ../wiki/Rocks/res/ | sed s/^/Rocks_/ > $GAMEDATA/resources.txt
for x in ../wiki/Rocks/res/*; do cp $x $GAMEDATA/Rocks_`basename $x`; done
